{
  "routes": [
    {
      "src": "/api/shorten",
      "dest": "/api/shorten.js"
    },
    {
      "src": "/api/info/([^/]+)",
      "dest": "/api/info/[code].js?code=$1"
    },
    {
      "src": "/([a-zA-Z0-9]{7})$",
      "dest": "/api/[code].js?code=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
